{
  "_id": "twelvedata-stock-raw",
  "type": "pipe",
  "source": {
    "type": "rest",
    "system": "twelvedata",
    "id_expression": "{{ exchange }}-{{ symbol }}",
    "operation": "stocks",
    "payload_property": "data",
    "rate_limiting_delay": 60,
    "rate_limiting_retries": 3
  },
  "pump": {
    "cron_expression": "0 6 * * ?"
  },
  "add_namespaces": false
}
