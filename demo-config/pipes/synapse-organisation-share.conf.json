{
  "_id": "synapse-organisation-share",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "synapse-organisation-transform"
  },
  "sink": {
    "type": "sql",
    "system": "synapse",
    "batch_size": 10000,
    "create_table_if_missing": true,
    "lenient": true,
    "primary_key": "_id",
    "recreate_table_on_first_run": true,
    "schema_definition": [{
      "type": "string",
      "name": "_id",
      "allow_null": false,
      "max_size": 4000,
      "min_size": 1,
      "primary_key": true,
      "source_property": "_id"
    }, {
      "type": "string",
      "name": "$ids",
      "allow_null": true,
      "max_size": 4000,
      "min_size": 1,
      "source_property": "$ids"
    }, {
      "type": "string",
      "name": "rdf:type",
      "allow_null": true,
      "max_size": 4000,
      "min_size": 1,
      "source_property": "rdf:type"
    }, {
      "type": "string",
      "name": "global-organisation:company-id",
      "allow_null": true,
      "max_size": 4000,
      "min_size": 1,
      "source_property": "global-organisation:company-id"
    }, {
      "type": "string",
      "name": "global-organisation:name",
      "allow_null": true,
      "max_size": 4000,
      "min_size": 1,
      "source_property": "global-organisation:name"
    }, {
      "type": "string",
      "name": "global-organisation:classification",
      "allow_null": true,
      "max_size": 4000,
      "min_size": 1,
      "source_property": "global-organisation:classification"
    }],
    "table": "global-organisation",
    "use_bulk_operations": true
  },
  "transform": {
    "type": "dtl",
    "rules": {
      "default": [
        ["discard",
          ["not", "_S._deleted"]
        ],
        ["copy", "*"]
      ]
    }
  },
  "batch_size": 10000,
  "remove_namespaces": false
}
